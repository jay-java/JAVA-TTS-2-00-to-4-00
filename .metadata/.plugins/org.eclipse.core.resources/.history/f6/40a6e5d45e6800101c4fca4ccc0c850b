package core;

public class P002_DataType {
	public static void main(String[] args) {
		// Primitive
		// byte,short,int,long
		// float,double
		// boolean,char

		byte b = 12;//1
		System.out.println(b);
		short s = 32767;//2
		int i = 12;//4
		long l = 12;//8
		float f = 3.14f;//4
		double d = 3.14;//8
		boolean flag = true;
		char c = '^';

	}
}
